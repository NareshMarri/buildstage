pipeline
{
    agent any
    stages{
        stage ('Build')
        {
            steps{
            echo "Building the application................"
            }
        }
        stage ('Test')
        {
            steps{
            echo "Testing the application....."
            }
        }
        stage ('QA')
        {
            steps{
            echo "QA is being done....."
                }
        }
        stage ('Deploy')
        {
            steps{
            echo "Deploying the application"
            }
        }
        stage ('Monitor')
        {
            steps{
            echo "Monitoring the application"
        
            }
            }
    }
}
